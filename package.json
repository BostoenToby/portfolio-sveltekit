{
  "name": "portfolio-svetlekit",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "start:prod": "git pull && npm i && npm run start",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.14"
  },
  "packageManager": "npm@8.19.3"
}
